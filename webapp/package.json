    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/kit": "^1.22.1",
    "@types/howler": "^2.2.7",
    "@types/lodash": "^4.14.195",
    "@types/nodemailer": "^6.4.13",
    "@types/ws": "^8.5.5",
